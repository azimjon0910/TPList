<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item
		android:width="25dp"
		android:height="25dp"
		tools:targetApi="m"
		tools:ignore="UnusedAttribute">
		<shape android:shape="oval">
			<solid android:color="#ffffff"/>
			<size
				android:width="25dp"
				android:height="25dp"/>
			<stroke
				android:width="0.6dp"
				android:color="#00AAFF" />
		</shape>
	</item>
	
	<item
		android:width="11dp"
		android:height="4dp"
		android:top="14dp"
		android:left="2dp"
		tools:targetApi="m"
		tools:ignore="UnusedAttribute">
		<rotate android:fromDegrees="40">
			<shape android:shape="rectangle">
				<solid android:color="#40A0A0"/>
			</shape>
		</rotate>
	</item>

	<item
		android:width="18dp"
		android:height="4dp"
		android:top="12dp"
		android:left="7dp"
		tools:ignore="UnusedAttribute">
		<rotate android:fromDegrees="-47">
			<shape android:shape="rectangle">
				<solid android:color="#40A0A0"/>
			</shape>
		</rotate>
	</item>
</layer-list>

